{
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "debug.allowBreakpointsEverywhere": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.suggest.showStatusBar": true,
  "editor.tokenColorCustomizations": {
    "[Quiet Light]": {
      "textMateRules": [
        {
          "name": "Diff: Header (Color From Solarized Light)",
          "scope": [
            "meta.diff.header",
            "meta.diff.header.from-file",
            "meta.diff.header.to-file",
            "meta.diff.range",
            "punctuation.definition.from-file.diff",
            "punctuation.definition.range.diff",
            "punctuation.definition.to-file.diff"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#268bd2"
          }
        },
        {
          "name": "Diff: Only In (Color From Solarized Light)",
          "scope": "meta.diff.only-in",
          "settings": {
            "foreground": "#268bd2"
          }
        },
        {
          "name": "Diff: Deleted (Color From Solarized Light)",
          "scope": [
            "markup.deleted",
            "punctuation.definition.deleted.diff"
          ],
          "settings": {
            "foreground": "#dc322f"
          }
        },
        {
          "name": "Diff: Inserted (Color From Solarized Light)",
          "scope": [
            "markup.inserted",
            "punctuation.definition.inserted.diff"
          ],
          "settings": {
            "foreground": "#219186"
          }
        }
      ]
    }
  },
  "files.hotExit": "off",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "html.format.wrapLineLength": 0,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.shell.windows": "C:/Program Files/Git/bin/bash.exe",
  "window.restoreWindows": "none",
  "workbench.colorTheme": "Quiet Light",
  "workbench.tree.indent": 24
}
