{
  "author": "Hugo Xu",
  "bugs": {
    "url": "https://github.com/jspenguin2017/RenovateConfig/issues"
  },
  "description": "Configurations for Renovate",
  "homepage": "https://github.com/jspenguin2017/RenovateConfig",
  "keywords": [
    "renovate",
    "config"
  ],
  "license": "GPL-3.0",
  "name": "renovate-config-hugoxu",
  "renovate-config": {
    "default": {
      "bumpVersion": "patch",
      "extends": [
        "config:base",
        ":assignee(jspenguin2017)",
        ":automergePatch",
        ":disableRateLimiting",
        ":enableVulnerabilityAlerts",
        ":includeNodeModules",
        ":maintainLockFilesWeekly",
        ":pinVersions",
        ":rebaseStalePrs",
        ":semanticPrefixChore",
        ":unpublishSafe"
      ],
      "recreateClosed": true,
      "rollbackPrs": true
    }
  },
  "repository": "https://github.com/jspenguin2017/RenovateConfig",
  "version": "1.0.2"
}
